  body
    //- // Spinner Start
    #spinner.show.bg-white.position-fixed.translate-middle.w-100.vh-100.top-50.start-50.d-flex.align-items-center.justify-content-center
      .spinner-border.text-primary(style='width: 3rem; height: 3rem;' role='status')
        span.sr-only Loading...
    //- // Spinner End
    //- // Navbar Start
    nav.navbar.navbar-expand-lg.bg-white.navbar-light.shadow.sticky-top.p-0
      a.navbar-brand.d-flex.align-items-center.px-4.px-lg-5(href='/index')
        h2.m-0.text-primary
          i.fa.fa-book.me-3
          | AfzaeLearning
      button.navbar-toggler.me-4(type='button' data-bs-toggle='collapse' data-bs-target='#navbarCollapse')
        span.navbar-toggler-icon
      #navbarCollapse.collapse.navbar-collapse
        .navbar-nav.ms-auto.p-4.p-lg-0
          a.nav-item.nav-link.active(href='/index') Home
          a.nav-item.nav-link(href='/about') About
          a.nav-item.nav-link(href='/course') Courses
          a.nav-item.nav-link(href='/contact') Contact
        a.btn.btn-primary.py-4.px-lg-5.d-none.d-lg-block(href='/signin')
          | Join Now
          i.fa.fa-arrow-right.ms-3
          //- Navbar end
    main
    block content    

    //-  Footer Start
    .container-fluid.bg-dark.text-light.footer.pt-5.mt-5.wow.fadeIn(data-wow-delay='0.1s')
      .container.py-5
        .row.g-5
          .col-lg-3.col-md-6
            h4.text-white.mb-3 Quick Link
            a.btn.btn-link(href='/about') About Us
            a.btn.btn-link(href='/contact') Contact Us
            a.btn.btn-link(href='/testimonial') Testimonials
            a.btn.btn-link(href='') Terms & Condition
            a.btn.btn-link(href='') FAQs & Help
          .col-lg-3.col-md-6
            h4.text-white.mb-3 Contact
            p.mb-2
              i.fa.fa-map-marker-alt.me-3
              | 3 Leyton Ave, Scarborough
            p.mb-2
              i.fa.fa-phone-alt.me-3
              | +1 647 780 3079
            p.mb-2
              i.fa.fa-envelope.me-3
              | info@afzaelearning.ca
            .d-flex.pt-2
              a.btn.btn-outline-light.btn-social(href='')
                i.fab.fa-twitter
              a.btn.btn-outline-light.btn-social(href='')
                i.fab.fa-facebook-f
              a.btn.btn-outline-light.btn-social(href='')
                i.fab.fa-youtube
              a.btn.btn-outline-light.btn-social(href='')
                i.fab.fa-linkedin-in
          .col-lg-3.col-md-6
            h4.text-white.mb-3 Gallery
            .row.g-2.pt-2.wow.fadeInUp(data-wow-delay='0.7s')
              .col-4
                img.img-fluid.bg-light.p-1(src='/images/course-1.jpg' alt='')
              .col-4
                img.img-fluid.bg-light.p-1(src='/images/course-2.jpg' alt='')
              .col-4
                img.img-fluid.bg-light.p-1(src='/images/course-3.jpg' alt='')
              //- .col-4
              //-   img.img-fluid.bg-light.p-1(src='img/course-2.jpg' alt='')
              //- .col-4
              //-   img.img-fluid.bg-light.p-1(src='img/course-3.jpg' alt='')
              //- .col-4
              //-   img.img-fluid.bg-light.p-1(src='img/course-1.jpg' alt='')
          .col-lg-3.col-md-6
            h4.text-white.mb-3 Newsletter
            p Please enter your email for subscription.
            .position-relative.mx-auto(style='max-width: 400px;')
              input.form-control.border-0.w-100.py-3.ps-4.pe-5(type='text' placeholder='Your email')
              button.btn.btn-primary.py-2.position-absolute.top-0.end-0.mt-2.me-2(type='button') SignUp
      .container
        .copyright
          .row
            .col-md-6.text-center.text-md-start.mb-3.mb-md-0
              | Â© Afza eLearning
              | , All Right Reserved.
              | Designed by 
              a.border-bottom(href='https://afzait.ca/') Abu Afza, AFZA IT
            .col-md-6.text-center.text-md-end
              .footer-menu
                a(href='/index') Home
                a(href='') Cookies
                a(href='') Help
                a(href='') FQAs
    //- // Footer End
    //- // Back to Top
    a.btn.btn-lg.btn-primary.btn-lg-square.back-to-top(href='#')
      i.bi.bi-arrow-up
